@model MVCApp1.Models.Registrado
@using MVCApp1.Models;
@{


    var direccion = new Direccion();

    //comunicacion http api rest
    //comunicacion a un metodos de una clase

    direccion.IdRegistrado = Model.IdRegistrado;
    direccion.Nombres = Model.NombresCompletos;


}



<h2>DIRECCIONES DEL REGISTRADO</h2>

<p>
    @Html.ActionLink("Nueva Direccion", "Create", "Direccion", new { Model.IdRegistrado })
</p>

<dd>
    @Html.DisplayFor(model => direccion.IdRegistrado)
</dd>


<dd>
    @Html.DisplayFor(model => direccion.Nombres)
</dd>
